    "type": "git",
    "url": "git+ssh://git@github.com:thekeogh/webpack-react.git"
  },
  "keywords": [
    "react",
    "webpack3",
    "boilerplate"
  ],
  "author": "Steve McKeogh",
  "bugs": {
    "url": "https://github.com/thekeogh/webpack-react/issues"
  },
  "homepage": "http://keogh.site",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
